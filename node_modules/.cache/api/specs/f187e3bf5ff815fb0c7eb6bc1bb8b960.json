          {
            "name": "inscriptionMin",
            "in": "query",
            "description": "Inscription number min",
            "required": false,
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "inscriptionMax",
            "in": "query",
            "description": "Inscription number max",
            "required": false,
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "satRarity",
            "in": "query",
            "description": "sat rarity",
            "required": false,
            "schema": {
              "type": "string",
              "enum": [
                "common",
                "uncommon",
                "rare",
                "epic",
                "legendary",
                "mythic"
